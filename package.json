    "reset:global": "npm uninstall -g @aneuhold/main-scripts && npm i -g @aneuhold/main-scripts"
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aneuhold/main-scripts.git"
  },
  "keywords": [
    "Scripting",
    "Node.js"
  ],
  "author": "Anton G. Neuhold Jr.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aneuhold/main-scripts/issues"
  },
  "homepage": "https://github.com/aneuhold/main-scripts#readme",
  "bin": {
    "tb": "./lib/src/index.js"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "leveldown": "^6.1.0",
    "levelup": "^5.0.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/leveldown": "^4.0.3",
    "@types/levelup": "^4.3.3",
    "@types/node": "^16.10.2",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
